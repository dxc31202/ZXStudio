  <data name="scintillaZ80Monitor1.ForceText" xml:space="preserve">
    <value>ORG #8000

screen EQU #4000
attr EQU 22528
START:
ld bc,#10
:loop
push bc
ld hl, screen       ;pixels
ld de, screen + 1   ;pixels + 1
ld bc, #FFF         ;pixels area length - 1
LD A,R
out (#fe),A
ld (hl), A          ;set first byte
ldir                ;copy bytes

ld hl, attr         ;attributes
ld de, attr + 1     ;attributes + 1
ld bc, #1FF         ;attribute area length - 1
ld (hl), A        ;set first
ldir                ;copy bytes

pop bc
DEC BC
LD A,B
OR C

jr nz loop

;OUT(#FE),A

ret</value>
  </data>
